<!-- 003.004  19-Jan-2011  Stig H. Eliasen                                                                   -->
<!--          Support for indication of BCD NSW                                                              -->
<!-- 003.003  07-May-2010  Stig H. Eliasen                                                                   -->
<!--          Support for traces                                                                             -->
<!-- 003.002  13-Aug-2009  Niels-Erik Ostergard                                                              -->
<!--          Support for OST resources and printf functionality                                             -->
<!-- 003.001  09-Mar-2009  Stig H. Eliasen, Niels-Erik Ostergard                                             -->
<!--          Changes due to review                                                                          -->
<!-- 003.000  12-Jan-2009  Stig H. Eliasen, Niels-Erik Ostergard                                             -->
<!-- 002.000  12-Apr-2007  Christian J. Hansen                                                               -->
<!--          Changes due to review                                                                          -->
<!-- 001.000  14-Dec-2006  Christian J. Hansen                                                               -->
<!--          First version of the file                                                                      -->
<!-- ======================================================================================================= -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <!-- Outer node. Must be there in order to support all resource types in same schema -->
  <xs:element name="AutogenXml">
    <xs:complexType>
      <xs:choice minOccurs="1" maxOccurs="1">
        <xs:element name="CommonSymbols" type="CommonSymbolsType" />
        <xs:element name="ResourceShared" type="ResourceSharedType" />
        <xs:element name="ResourceIsi" type="ResourceIsiType" />
        <xs:element name="ResourceIsiExtension" type="ResourceIsiType" />
        <xs:element name="ResourceIsiCommon" type="ResourceIsiType" />
        <xs:element name="ResourceIsiCommonExtension" type="ResourceIsiPrefixType" />
        <xs:element name="ResourceIsiPrivate" type="ResourceIsiPrivateType" />
        <xs:element name="ResourceIsiPrefix" type="ResourceIsiPrefixType" />
        <xs:element name="ResourcePhonet" type="ResourcePhonetType" />
        <xs:element name="ResourcePhonetExtended" type="ResourcePhonetType" />
        <xs:element name="ResourceOst" type="ResourceOstType" />
        <xs:element name="ResourceMediaModule" type="ResourceMediaModuleType" />
        <xs:element name="ResourceMediaModuleExtension" type="ResourceMediaModuleExtensionType" />
        <xs:element name="ResourceGenericProtocol" type="ResourceGenericProtocolType" />
        <xs:element name="ResourceGenericProtocolExtension" type="ResourceGenericProtocolType" />
        <xs:element name="ResourceInvariantTraces" type="ResourceInvariantTracesType" />
        <xs:element name="ResourceTraceSymbian" type="ResourceTraceSymbianType" />
        <xs:element name="ResourceVendorComponentTraces" type="ResourceVendorComponentTracesType" />
        <xs:element name="ResourceTraceMcu" type="ResourceTraceMcuType" />
        <xs:element name="ResourceTraceOs" type="ResourceTraceOsType" />
        <xs:element name="ResourceTraceHal" type="ResourceTraceHalType" />
        <xs:element name="ResourceTraceMaster" type="ResourceTraceMasterType" />
        <xs:element name="ResourceTraceCdsp5" type="ResourceTraceCdsp5Type" />
        <xs:element name="ResourceTraceAdsp5" type="ResourceTraceAdsp5Type" />
        <xs:element name="ResourceTraceHsp" type="ResourceTraceHspType" />
      </xs:choice>
      <xs:attribute name="Version" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>
  <!-- =====================================================================================-->
  <!-- Common types -->
  <!-- =====================================================================================-->
  <xs:attributeGroup name="VersionInfoGroup">
    <xs:attribute name="ValidFrom" type="VersionType" use="optional" />
    <xs:attribute name="ObsoleteFrom" type="VersionType" use="optional" />
  </xs:attributeGroup>
  <!-- Feature Flag type -->
  <xs:complexType name="FeatureFlagType">
    <xs:sequence>
      <xs:element name="Operand" type="FeatureFlagOperandType" minOccurs="1" maxOccurs="2" />
    </xs:sequence>
    <xs:attribute name="Operator" use="optional">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="And" />
          <xs:enumeration value="Or" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="Negate" type="xs:token" fixed="True" use="optional" />
  </xs:complexType>
  <!-- Feature Flag operand type -->
  <xs:complexType name="FeatureFlagOperandType">
    <xs:simpleContent>
      <xs:extension base="NullType">
        <xs:attribute name="Name" type="xs:string" use="required" />
        <xs:attribute name="Value" type="xs:string" use="required" />
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <!-- =====================================================================================-->
  <!-- Description types -->
  <!-- =====================================================================================-->
  <xs:complexType name="DescriptionType">
    <xs:sequence minOccurs="0" maxOccurs="unbounded">
      <xs:choice>
        <xs:element name="Break" type="NullType" />
        <xs:element name="Para" type="xs:string" />
        <xs:element name="Bullet" type="xs:string" />
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DescriptionNotesType">
    <xs:sequence minOccurs="0" maxOccurs="unbounded">
      <xs:element name="Note">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="1">
            <xs:element name="Description" type="DescriptionType" />
          </xs:sequence>
          <xs:attribute name="Name" type="xs:string" use="required" />
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <!-- =====================================================================================-->
  <!-- Project info types -->
  <!-- =====================================================================================-->
  <xs:complexType name="ProjectInfoType">
    <xs:sequence>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element name="NokiaEntity" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="Confidentiality" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="SiteName" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="ProjectName" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="ProjectOwner" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="IsiVersionAutoGenerate" type="VersionType" minOccurs="0" maxOccurs="1" />
        <xs:element name="CopyrightYear" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="AlignmentCheckBreak" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="AlignmentCheckDisable" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="InheritBaseResourceDefinitions" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="IsiVersioningNotSupported" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="Nokia3rdPartyVendorOutput" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="ProtocolHasNoMessages" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="SubmessageNamesAreUnique" type="NullType" minOccurs="0" maxOccurs="1" />
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <!-- =====================================================================================-->
  <!-- ISI header information types -->
  <!-- =====================================================================================-->
  <xs:complexType name="IsihdrInfoType">
    <xs:sequence>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element name="AliasName" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="ContinuusInstanceName" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="ContinuusInstanceName16" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="ContinuusProjectDb" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="ContinuusProjectPath" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="ContinuusProjectPath16" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="DefinitionName" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="Filename" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="Filename16" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="Title" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="MergedMaskPrefix" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="PhonetHeaderLayout" type="EPhonetHeaderLayoutType" minOccurs="0" maxOccurs="1" />
        <xs:element name="StructName" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="StructLengthValue" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="StructLengthName" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="StructPhonetLengthName" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="Varname16" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="IsiHeaderTop" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="IsiHeaderTop16" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="IsiHeaderBeforeDef" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="IsiHeaderBeforeDef16" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="IsiHeaderAfterDef" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="IsiHeaderAfterDef16" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="GenerationType" type="EIsihdrGenerationType" minOccurs="0" maxOccurs="1" />
        <xs:element name="PointerGenerationType" type="EPointerGenerationType" minOccurs="0" maxOccurs="1" />
        <xs:element name="GenerateAllStructureElements" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="GenerateAsBitfield" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="GenerateAsBool" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="GenerateAsEnum8" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="GenerateAsEnum16" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="GenerateBitfields" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="GenerateBools" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="GenerateMergedMasks" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="GenerateMergedMasks16" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="IncludePhonetHeader" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="IncludePhonetHeader16" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="PhonetHeaderDontGenerate" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="SplitAsArray" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="SplitDwords16" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="SplitDwordSigned" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="SplitDwordUnsigned" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="SplitWordSigned" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="SplitWordUnsigned" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="StructLengthSizeof" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="StructLengthSum" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="DontGenerateId" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="DontGenerate" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="DontGenerate8" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="DontGenerate16" type="NullType" minOccurs="0" maxOccurs="1" />
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <!-- =====================================================================================-->
  <!-- HTML information types -->
  <!-- =====================================================================================-->
  <xs:complexType name="HtmlInfoType">
    <xs:sequence>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element name="ContinuusProjectDb" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="ContinuusProjectName" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="DirectoryPath" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="DocumentNumber" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="Filename" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="Title" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="IncludeVarnameColumn" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="IncludeIntroduction" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="IncludePhonetHeader" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="DontGenerate" type="NullType" minOccurs="0" maxOccurs="1" />
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <!-- =====================================================================================-->
  <!-- PMD information types -->
  <!-- =====================================================================================-->
  <xs:complexType name="PmdInfoType">
    <xs:sequence>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element name="PmdPhonetDeviceId" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="PmdPhonetObjectId" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="EnvironmentVariable" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="MessageInSummary" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="PmdIsNotParameter" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="PmdParameter" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="ParameterInSummary" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="UseConstTblComment" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="TraceInSummary" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="TraceActivationDefaultStateOn" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="TraceActivationExclude" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="TraceActivationText" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="TraceIdentifier" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="TraceLogicalGroup" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="TraceSource" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="TraceSourceFlags" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="TraceSourceIdentifier" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="TraceSourceName" type="xs:string" minOccurs="0" maxOccurs="1" />
        <xs:element name="DontGenerate" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="HardwareMasterTrace" type="NullType" minOccurs="0" maxOccurs="1" />
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <!-- =====================================================================================-->
  <!-- Version history types -->
  <!-- =====================================================================================-->
  <xs:complexType name="VersionHistoryType">
    <xs:sequence>
      <xs:element name="VersionInfo" type="VersionInfoType" minOccurs="1" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="VersionInfoType">
    <xs:sequence>
      <xs:element name="IsihdrInfo" type="IsihdrInfoType" minOccurs="0" maxOccurs="1" />
      <xs:element name="HtmlInfo" type="HtmlInfoType" minOccurs="0" maxOccurs="1" />
      <xs:element name="Reason" type="xs:string" minOccurs="1" maxOccurs="1" />
      <xs:element name="Reference" type="xs:string" minOccurs="1" maxOccurs="unbounded" />
      <xs:choice minOccurs="0" maxOccurs="1">
        <xs:element name="NotYetSupported" type="NullType" minOccurs="0" maxOccurs="1" />
        <xs:element name="NoLongerSupported" type="NullType" minOccurs="0" maxOccurs="1" />
      </xs:choice>
      <xs:element name="SuppressAutogenWarnings" type="NullType" minOccurs="0" maxOccurs="1" />
      <xs:element name="Description" type="DescriptionType" minOccurs="0" maxOccurs="1" />
    </xs:sequence>
    <xs:attribute name="Version" type="xs:string" use="required" />
    <xs:attribute name="Status" type="EVersionStates" use="required" />
    <!-- Should be changed to DateType later on... -->
    <xs:attribute name="Date" type="xs:string" use="required" />
    <xs:attribute name="Editor" type="xs:string" use="required" />
  </xs:complexType>
  <xs:simpleType name="EVersionStates">
    <xs:restriction base="xs:token">
      <xs:enumeration value="Unknown" />
      <xs:enumeration value="Draft" />
      <xs:enumeration value="NonDraft" />
      <xs:enumeration value="Proposal" />
      <xs:enumeration value="Approved" />
    </xs:restriction>
  </xs:simpleType>
  <!-- =====================================================================================-->
  <!-- Sequence item groups -->
  <!-- =====================================================================================-->
  <xs:group name="ArrayItemsGroup">
    <xs:sequence>
      <xs:choice>
        <xs:element name="Alignment" type="AlignmentType" />
        <xs:element name="Array" type="ArrayType" />
        <xs:element name="Filler" type="FillerType" />
        <xs:element name="IrregularStructure" type="IrregularStructureType" />
        <xs:element name="Number" type="NumberType" />
        <xs:element name="String" type="StringType" />
        <xs:element name="StructureReference" type="StructureReferenceType" />
        <xs:element name="SubblockReference" type="SubblockReferenceType" />
        <xs:element name="PhonetDevice" type="NumberType" />
        <xs:element name="PhonetObject" type="SpecialNumberType" />
        <xs:element name="MediaId" type="SpecialNumberType" />
      </xs:choice>
    </xs:sequence>
  </xs:group>
  <xs:group name="IrregularStructureItemsGroup">
    <xs:sequence>
      <xs:choice>
        <xs:element name="Filler" type="FillerType" />
        <xs:element name="Number" type="NumberType" />
        <xs:element name="PhonetDevice" type="NumberType" />
        <xs:element name="PhonetObject" type="SpecialNumberType" />
        <xs:element name="MediaId" type="SpecialNumberType" />
        <xs:element name="TraceGroupId" type="SpecialNumberType" />
        <xs:element name="TraceId" type="SpecialNumberType" />
      </xs:choice>
    </xs:sequence>
  </xs:group>
  <xs:group name="SequenceItemsGroup">
    <xs:sequence>
      <xs:choice>
        <xs:element name="Alignment" type="AlignmentType" />
        <xs:element name="Array" type="ArrayType" />
        <xs:element name="Filler" type="FillerType" />
        <xs:element name="IrregularStructure" type="IrregularStructureType" />
        <xs:element name="Number" type="NumberType" />
        <xs:element name="Optional" type="OptionalType" />
        <xs:element name="ProtocolReference" type="ProtocolReferenceType" />
        <xs:element name="ExternalDecoder" type="ExternalDecoderType" />
        <xs:element name="PhonetMessageBlock" type="PhonetMessageBlockType" />
        <xs:element name="PmdAnchor" type="PmdAnchorType" />
        <xs:element name="RawData" type="RawDataType" />
        <xs:element name="XFile" type="XFileType" />
        <xs:element name="Selection" type="SelectionType" />
        <xs:element name="String" type="StringType" />
        <xs:element name="StructureReference" type="StructureReferenceType" />
        <xs:element name="SubblockList" type="SubblockListType" />
        <xs:element name="SubblockIdList" type="SubblockListType" />
        <xs:element name="SubblockReference" type="SubblockReferenceType" />
        <xs:element name="PhonetDevice" type="NumberType" />
        <xs:element name="PhonetObject" type="SpecialNumberType" />
        <xs:element name="MediaId" type="SpecialNumberType" />
        <xs:element name="TraceDataLength" type="SpecialNumberType" />
      </xs:choice>
    </xs:sequence>
  </xs:group>
  <xs:group name="TraceSequenceItemsGroup">
    <xs:sequence>
      <xs:choice>
        <!-- Printf is only allowed in traces and must be placed outside SequenceItemsGroup. -->
        <!-- Placement of printf is not fixed among the items. Note that only one printf is  -->
        <!-- allowed per trace. This requirement is not catched by the schema.               -->
        <xs:element name="Printf" type="PrintfType" />
        <xs:element name="McuFunctionAddress" type="SpecialNumberType" />
        <!-- Include all common sequence items -->
        <xs:group ref="SequenceItemsGroup" />
      </xs:choice>
    </xs:sequence>
  </xs:group>
  <xs:group name="SubblockItemsGroup">
    <xs:sequence>
      <xs:choice>
        <!-- As sub block length is optional and placement is not fixed, -->
        <!-- sub block length has to be placed in this list -->
        <xs:element name="SubblockLength" type="SpecialNumberType" />
        <xs:element name="TraceEntityId" type="SpecialNumberType" />
        <xs:element name="TraceGroupId" type="SpecialNumberType" />
        <xs:element name="TraceId" type="SpecialNumberType" />
        <!-- Include all common sequence items -->
        <xs:group ref="SequenceItemsGroup" />
      </xs:choice>
    </xs:sequence>
  </xs:group>
  <xs:group name="PrintfItemsGroup">
    <xs:sequence>
      <xs:choice>
        <xs:element name="Number" type="NumberType" />
        <xs:element name="String" type="StringType" />
        <xs:element name="Filler" type="FillerType" />
        <xs:element name="Alignment" type="AlignmentType" />
      </xs:choice>
    </xs:sequence>
  </xs:group>
  <xs:group name="ProtocolMessageItemsGroup">
    <xs:sequence>
      <xs:choice>
        <!-- Protocol message specific sequence items -->
        <xs:element name="MessageId" type="SpecialNumberType" />
        <xs:element name="SubmessageId" type="SpecialNumberType" />
        <xs:element name="MessageLength" type="SpecialNumberType" />
        <!-- Include all common sequence items -->
        <xs:group ref="SequenceItemsGroup" />
      </xs:choice>
    </xs:sequence>
  </xs:group>
  <!-- =====================================================================================-->
  <!-- Count types -->
  <!-- =====================================================================================-->
  <xs:complexType name="CountType">
    <xs:simpleContent>
      <xs:extension base="NullType">
        <xs:attribute name="Type" type="ECountAlgorithmTypes" use="required" />
        <xs:attribute name="TagName" type="DefinitionNameType" use="optional" />
        <xs:attribute name="Value" type="xs:token" use="optional" default="0" />
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <!-- Count algorithm types  -->
  <xs:simpleType name="ECountAlgorithmTypes">
    <xs:restriction base="xs:token">
      <xs:enumeration value="Fixed" />
      <xs:enumeration value="TagValue" />
      <xs:enumeration value="TagValuePlusValue" />
      <xs:enumeration value="ValueMinusTagValue" />
      <xs:enumeration value="ValueMultipliedWithTagValue" />
      <xs:enumeration value="TagValueDividedWithValue" />
      <xs:enumeration value="TagValueDividedWithValueRoundedUp" />
      <xs:enumeration value="AsManyAsPossible" />
      <xs:enumeration value="ZeroTerminated" />
      <xs:enumeration value="Unknown" />
    </xs:restriction>
  </xs:simpleType>
  <!-- =====================================================================================-->
  <!-- Common symbols type -->
  <!-- =====================================================================================-->
  <xs:complexType name="CommonSymbolsType">
    <xs:sequence>
      <xs:element name="Tables" type="TablesType" minOccurs="0" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>
  <!-- =====================================================================================-->
  <!-- Message resource types -->
  <!-- =====================================================================================-->
  <!-- Resource base type (used as base for most resource types) -->
  <xs:complexType name="BaseResourceType" abstract="true">
    <xs:sequence>
      <xs:element name="Description" type="DescriptionType" minOccurs="0" maxOccurs="1" />
      <xs:element name="ProjectInfo" type="ProjectInfoType" minOccurs="0" maxOccurs="1" />
      <xs:element name="IsihdrInfo" type="IsihdrInfoType" minOccurs="0" maxOccurs="1" />
      <xs:element name="HtmlInfo" type="HtmlInfoType" minOccurs="0" maxOccurs="1" />
      <xs:element name="PmdInfo" type="PmdInfoType" minOccurs="0" maxOccurs="1" />
      <xs:element name="SuppressAutogenWarnings" type="NullType" minOccurs="0" maxOccurs="1" />
      <xs:element name="DescriptionNotes" type="DescriptionNotesType" minOccurs="0" maxOccurs="1" />
      <xs:element name="VersionHistory" type="VersionHistoryType" minOccurs="0" maxOccurs="1" />
      <xs:element name="SharedFiles" type="SharedFilesType" minOccurs="0" maxOccurs="1" />
